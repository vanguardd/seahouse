  <sql id="houseListColumn">
    id, room_image, title, fixtures, room_area, rent, rent_way, first_address, labels, address, address_coordinate
  </sql>

  <!--房屋搜索筛选-->
  <select id="search" resultMap="houseListResultMap" parameterType="com.team.seahouse.commons.request.SearchQuery" >
    SELECT <include refid="houseListColumn" />
      FROM tb_house
    <where>
      <if test="keyWord != null and keyWord != ''">
        AND title LIKE '%${keyWord}%' OR address LIKE '%${keyWord})%'
      </if>
      <if test="type != null">
        AND type = #{type}
      </if>
      <if test="exposition != null and exposition != ''">
        AND exposition = #{exposition}
      </if>
      <if test="minPrice != null and minPrice != 0 and maxPrice != null and maxPrice != 0">
        AND rent BETWEEN #{minPrice} AND #{maxPrice}
      </if>
      <if test="minPrice != null and minPrice != 0 and maxPrice != null and maxPrice != 0">
        AND rent &gt; #{minPrice}
      </if>
    </where>
  </select>

  <!--根据房屋编号查询房屋详情-->
  <select id="findByHouseId" resultMap="houseDetailResultMap" parameterType="Long">
    select h.id, h.title, h.house_name, h.living_room,
           h.living_room_images, h.bath_room, h.bath_room_images,
           h.kitchen, h.kitchen_images, h.balcony, h.balcony_images,
           h.property_value, h.property_pay_type, h.water_value, h.water_pay_type,
           h.electric_value, h.electric_pay_type, h.heating_value, h.heating_pay_type,
           h.custom_name, h.custom_value, h.custom_pay_type, h.floor,
           h.is_has_elevator, h.fixtures, h.house_pattern, h.room_name, h.room_area,
           h.room_facilities, h.room_image, h.room_images, h.exposition,
           h.rent, h.rent_way, h.pay_way, h.labels, h.introduction, h.address,
           h.address_coordinate, h.first_address, h.plot_name, h.green_area,
           h.heating_way, h.parking, h.property, h.years, h.village_introduction,
           h.rent_rule, h.exit_rent_rule, h.landlord_id, h.land_lard_zhi_ma_score,
           u.real_name, u.mobile_phone, u.create_time
      from tb_house h, tb_user_info u
      where h.landlord_id = u.user_id and h.id=#{houseId}
  </select>

  <!--根据房屋编号集合查询房屋列表-->
  <select id="findByHouseIdIn" resultMap="houseListResultMap">
    SELECT <include refid="houseListColumn" />
      FROM tb_house
    WHERE id IN 
    <foreach collection="houseIds" index="index" item="item" open="(" separator="," close=")">
      #{item}
    </foreach>
  </select>

  <!-- 根据用户编号查询我的收藏房屋列表 -->
  <select id="findCollectedHouseByUserId" resultMap="houseListResultMap">
    SELECT <include refid="houseListColumn" />
      FROM tb_house h
    LEFT JOIN tb_collection c
    ON h.id = c.house_id
    WHERE c.user_id=#{userId}
  </select>

  <!--根据房屋类型查询房屋列表-->
  <select id="findHousesByType" resultMap="houseListResultMap">
    SELECT <include refid="houseListColumn" />
      FROM tb_house
    WHERE type=#{type}
  </select>

